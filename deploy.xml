<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
	<global>
		<id>arclibrary</id>
		<version>1.6.0</version>
		<patchlevel>0</patchlevel>
		<vendor>ontoprise GmbH</vendor>
		<maintainer>ontoprise GmbH</maintainer>
		<instdir>extensions/ARCLibrary</instdir>
		<description>
			Provides the ARC library to other MW extensions.
		</description>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:ARCLibrary_extension</helpurl>
		<dependencies>
			<dependency from="1.16.0">mw</dependency>
		</dependencies>
		<license>GPL v2</license>
	</global>
	
	<codefiles>
		<file loc="deploy.xml" />
	</codefiles>
	
	<wikidumps>
		<!-- empty -->
	</wikidumps>
	
	<resources>
		<!-- empty -->
	</resources>

	<configs>
        <successor ext="smw"/>
		<new>
			<require name="arclib_init"
				file="extensions/ARCLibrary/ARCLibrary.php" />
			<function name="enableARCLibrary">
			</function>
		</new>
		<uninstall>
			<!-- empty -->
		</uninstall>
	</configs>
</deploydescriptor>

